{
  "name": "eslint-snippets",
  "version": "0.0.14",
  "description": "Snippets of all eslint rules available",
  "keywords": [
    "atom",
    "eslint",
    "snippets",
    "completion",
    "autocomplete"
  ],
  "scripts": {
    "build": "node .bin/build.js",
    "__preversion": "git fetch",
    "__postversion": "git push origin master && git push origin $(git describe --abbrev=0 --tags)"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "cson": "^4.1.0",
    "eslint": "^4.1.1"
  },
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamataryo/eslint-snippets.git"
  },
  "author": "kamataryo <mugil.cephalus+gitgub.com@gmail.com>",
  "bugs": {
    "url": "https://github.com/kamataryo/eslint-snippets/issues"
  },
  "homepage": "https://github.com/kamataryo/eslint-snippets#readme"
}
